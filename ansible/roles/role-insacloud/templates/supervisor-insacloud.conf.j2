[program:{{insacloud_appname}}]
command = {{insacloud_virtualenv_path}}/bin/gunicorn_start            ; Command to start app
user = root
stdout_logfile = /var/log/supervisor/{{insacloud_appname}}.log        ; Where to write log messages
redirect_stderr = true                                                ; Save stderr in the same log
environment=LANG=en_US.UTF-8,LC_ALL=en_US.UTF-8                       ; Set UTF-8 as default encoding